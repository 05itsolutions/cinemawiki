{
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery"
	],
	"globals": {
		"require": "readonly",
		"module": "readonly",
		"mw": "readonly",
		"OO": "readonly"
	},
	"rules": {
		"quote-props": [ "error", "as-needed" ],
		"max-len": "off",
		"no-jquery/no-global-selector": "off",
		"mediawiki/msg-doc": "error"
	},
	"plugins": [ "mediawiki" ]
}
